    "node": ">= 0.8.0"
  },
  "dependencies": {
    "gulp-util": "^3.0.0",
    "through2": "^0.6.1",
    "readline-sync": "~1.2.14"
  },
  "homepage": "https://github.com/anseki/gulp-confirm",
  "repository": {
    "type": "git",
    "url": "git://github.com/anseki/gulp-confirm.git"
  },
